<%- include('../header') %>

<!-- wrapper  -->

<div class="dashboard-wrapper">
	<div class="dashboard-ecommerce">
		<div class="container-fluid dashboard-content ">

			<!-- pageheader  -->

			<div class="row">
				<div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
					<div class="page-header">
						<h2 class="pageheader-title">User</h2>

						<div class="page-breadcrumb">
							<nav aria-label="breadcrumb">
								<ol class="breadcrumb">
									<li class="breadcrumb-item"><a href="/api/admin/all-users" class="breadcrumb-link">Dashboard</a>
									</li>
									<li class="breadcrumb-item active" aria-current="page">User</li>
								</ol>
							</nav>
						</div>
					</div>
				</div>
			</div> 
			<!-- end pageheader  -->

<!-- <p><%= JSON.toString(user) %> </p> -->

			<div class="row">

				<!-- profile -->

				<div class="col-xl-3 col-lg-3 col-md-5 col-sm-12 col-12">

					<!-- card profile -->
          
					<div class="card <%= user.isAdmin? "border border-primary": "" %> ">
						<img class="card-img-top" 
						style="height: 35vh; object-fit: cover;"
						src="<%= hostname + "/"+ user.profile_img %>" alt="Card image cap">
						<div class="card-body">
							<div class="text-center">
								<h2 class="font-24 mb-0"><%=user.username%></h2>
								<h4 class="mb-0"><%=user.first_name+" "+user.last_name%></h4>
							</div>
            </div>
            <div class="card-body border-top">
							<h3 class="font-16">FireBase Auth ID</h3>
							<div class="card-text">
								<%= user.authID %> 
							</div>
						</div>
						<div class="card-body border-top">
							<h3 class="font-16">Email</h3>
							<div class="card-text">
								<%= user.email %> 
							</div>
						</div>
						
						<div class="card-body border-top">
							<h3 class="font-16">Description</h3>
							<div class="card-text">
								<%= user.about_me %> 
							</div>
						</div>
						<div class="card-body border-top">
							<h3 class="font-16">Genres</h3>
							<div class="card-text">
                <% if(user.genres){
                  user.genres.forEach( (genre) => { %>
                  <%= genre+", " %> 
                <% })} else { %> 
                  None
                <% } %>
							</div>
						</div>
						<div class="row text-center border-top">
							<div class="col-6 p-2 border-right border-top border-bottom">
								<h6 class="">Location</h6>
								<h3 class="mb-2"><%= user.city +", " + user.state+", "+user.country%></h3>
							</div>
							<div class="col-6 p-2 border-right border-top border-bottom">
								<h6 class="">Age</h6>
								<h3 class="mb-2"><%= user.age%></h3>
							</div>		
							<div class="col-6 p-2 border-right border-top border-bottom">
								<h6 class="">Gender</h6>
								<h3 class="mb-2"><%= user.gender%></h3>
              </div>
              <div class="col-6 p-2 border-right border-top border-bottom">
								<h6 class="">Identity</h6>
								<h3 class="mb-2"><%= user.identity%></h3>
              </div>
              <div class="col-6 p-2 border-right border-top border-bottom">
								<h6 class="">Preference</h6>
								<h3 class="mb-2"><%= user.preference%></h3>
              </div>
              <div class="col-6 p-2 border-right border-top border-bottom">
								<h6 class="">Looking For</h6>
								<h3 class="mb-2"><%= user.looking_for%></h3>
              </div>
              <div class="col-6 p-2 border-right border-top border-bottom">
								<h6 class="">Ethnicity</h6>
								<h3 class="mb-2"><%= user.ethnicity%></h3>
              </div>
              <div class="col-6 p-2 border-right border-top border-bottom">
								<h6 class="">Drink/Party</h6>
								<h3 class="mb-2"><%= user.drink_party%></h3>
              </div>
              <div class="col-6 p-2 border-right border-top border-bottom">
								<h6 class="">Star Sign</h6>
								<h3 class="mb-2"><%= user.star_sign%></h3>
              </div>
              <div class="col-6 p-2 border-right border-top border-bottom">
								<h6 class="">Creative</h6>
								<h3 class="mb-2"><%= user.creative%></h3>
							</div>
						</div>    
						<div class="card-body border-top">
							<div class="row text-center justify-content-end">
								<div class="col-6">
									<td><a href="/api/admin/delete-user/<%= user._id %>" class="btn btn-danger w-100">Delete</a></td>
								</div>
							</div>
						</div>
						      
					</div>
				</div>       
                <!-- end profile -->
                

               
                        <!-- campaign data -->
                        
                        <div class="col-xl-9 col-lg-9 col-md-7 col-sm-12 col-12">
                            
                            <!-- campaign tab one -->
                            
                            <div class="influence-profile-content pills-regular">
                                <ul class="nav nav-pills mb-3 nav-justified" id="pills-tab" role="tablist">
                                    <li class="nav-item">
																				<a class="nav-link active"
																				  id="pills-tickets-tab"
																					data-toggle="pill" 
																					href="#pills-tickets" 
																					role="tab" 
																					aria-controls="pills-tickets" 
																					aria-selected="true">Social</a>
                                    </li>
                                   
                                    <li class="nav-item">
																				<a class="nav-link" 
																				id="pills-details-tab" 
																				data-toggle="pill" 
																				href="#pills-details" 
																				role="tab" 
																				aria-controls="pills-details" 
																				aria-selected="false">Tickets</a>
                                    </li>
                                    <li class="nav-item">
																				<a class="nav-link" 
																				id="pills-guests-tab" 
																				data-toggle="pill" 
																				href="#pills-guests" 
																				role="tab" 
																				aria-controls="pills-guests" 
																				aria-selected="false">Events</a>
                                    </li>
																</ul>
                                <div class="tab-content" id="pills-tabContent">
                                    <div class="tab-pane fade show active" id="pills-tickets" role="tabpanel" aria-labelledby="pills-tickets-tab">
                                        <!-- ==================================================== -->
																							<!-- Friends -->
																				<!-- ==================================================== -->
                                        <div class="row">
																					<div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
																						<div class="card">
																								<h5 class="card-header">Friends</h5>
																								<div class="card-body p-0">
																										<div class="table-responsive">
																												<table class="table">
																														<thead class="bg-light">
																																<tr class="border-0">
																																		<th class="border-0">#</th>
																																		<th class="border-0">Name</th>
																																		<th class="border-0">Gender</th>
																																		<th class="border-0">Email</th>
																																		<th class="border-0">Preference</th>
																																		
																																</tr>
																														</thead>
																														<tbody>
																															<% if(user.friends){
																																user.friends.forEach((item, i) => { %> 
																																<tr>
																																		<td><%= i %> </td>
																																		<td><a href="/api/admin/user/<%= item._id %>  "><%= item.username %> </a></td>
																																		<td><%= item.gender %> </td>
																																		<td><%= item.email %> </td>
																																		<td><%= item.preference %> </td>
																																</tr>
																															<% })}%>
																																<!-- <tr>
																																		<td colspan="8"><a href="#" class="btn btn-outline-light float-right">View Details</a></td>
																																</tr> -->
																														</tbody>
																												</table>
																										</div>
																								</div>
																						</div>
																					</div>
                                          
                                          <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
																						<div class="card">
																								<h5 class="card-header">Requests Sent</h5>
																								<div class="card-body p-0">
																										<div class="table-responsive">
																												<table class="table">
																														<thead class="bg-light">
																																<tr class="border-0">
																																		<th class="border-0">#</th>
																																		<th class="border-0">Name</th>
																																		<th class="border-0">Gender</th>
																																		<th class="border-0">Email</th>
																																		<th class="border-0">Preference</th>
																																		
																																</tr>
																														</thead>
																														<tbody>
																															<% if(user.requests_sent){
																																user.requests_sent.forEach((item, i) => { %> 
																																<tr>
																																		<td><%= i %> </td>
																																		<td><a href="/api/admin/user/<%= item._id %>  "><%= item.username %> </a></td>
																																		<td><%= item.gender %> </td>
																																		<td><%= item.email %> </td>
																																		<td><%= item.preference %> </td>
																																</tr>
																															<% })}%>
																																<!-- <tr>
																																		<td colspan="8"><a href="#" class="btn btn-outline-light float-right">View Details</a></td>
																																</tr> -->
																														</tbody>
																												</table>
																										</div>
																								</div>
																						</div>
																					</div>
                                          
                                          <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
																						<div class="card">
																								<h5 class="card-header">Requests Pending</h5>
																								<div class="card-body p-0">
																										<div class="table-responsive">
																												<table class="table">
																														<thead class="bg-light">
																																<tr class="border-0">
																																		<th class="border-0">#</th>
																																		<th class="border-0">Name</th>
																																		<th class="border-0">Gender</th>
																																		<th class="border-0">Email</th>
																																		<th class="border-0">Preference</th>
																																		
																																</tr>
																														</thead>
																														<tbody>
																															<% if(user.requests_pending){
																																user.requests_pending.forEach((item, i) => { %> 
																																<tr>
																																		<td><%= i %> </td>
																																		<td><a href="/api/admin/user/<%= item._id %>  "><%= item.username %> </a></td>
																																		<td><%= item.gender %> </td>
																																		<td><%= item.email %> </td>
																																		<td><%= item.preference %> </td>
																																</tr>
																															<% })}%>
																																<!-- <tr>
																																		<td colspan="8"><a href="#" class="btn btn-outline-light float-right">View Details</a></td>
																																</tr> -->
																														</tbody>
																												</table>
																										</div>
																								</div>
																						</div>
																					</div>
                                          
                                          <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
																						<div class="card">
																								<h5 class="card-header">Blocked</h5>
																								<div class="card-body p-0">
																										<div class="table-responsive">
																												<table class="table">
																														<thead class="bg-light">
																																<tr class="border-0">
																																		<th class="border-0">#</th>
																																		<th class="border-0">Name</th>
																																		<th class="border-0">Gender</th>
																																		<th class="border-0">Email</th>
																																		<th class="border-0">Preference</th>
																																		
																																</tr>
																														</thead>
																														<tbody>
																															<%  if(user.blocked){
																																user.blocked.forEach((item, i) => { %> 
																																<tr>
																																		<td><%= i %> </td>
																																		<td><a href="/api/admin/user/<%= item._id %>  "><%= item.username %> </a></td>
																																		<td><%= item.gender %> </td>
																																		<td><%= item.email %> </td>
																																		<td><%= item.preference %> </td>
																																</tr>
																															<% })}%>
																																<!-- <tr>
																																		<td colspan="8"><a href="#" class="btn btn-outline-light float-right">View Details</a></td>
																																</tr> -->
																														</tbody>
																												</table>
																										</div>
																								</div>
																						</div>
																					</div>
                                          
                                          <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
																						<div class="card">
																								<h5 class="card-header">Reported</h5>
																								<div class="card-body p-0">
																										<div class="table-responsive">
																												<table class="table">
																														<thead class="bg-light">
																																<tr class="border-0">
																																		<th class="border-0">#</th>
																																		<th class="border-0">Name</th>
																																		<th class="border-0">Gender</th>
																																		<th class="border-0">Email</th>
																																		<th class="border-0">Preference</th>
																																		
																																</tr>
																														</thead>
																														<tbody>
																															<%  if(user.reported){
																																user.reported.forEach((item, i) => { %> 
																																<tr>
																																		<td><%= i %> </td>
																																		<td><a href="/api/admin/user/<%= item._id %>  "><%= item.username %> </a></td>
																																		<td><%= item.gender %> </td>
																																		<td><%= item.email %> </td>
																																		<td><%= item.preference %> </td>
																																</tr>
																															<% })}%>
																																<!-- <tr>
																																		<td colspan="8"><a href="#" class="btn btn-outline-light float-right">View Details</a></td>
																																</tr> -->
																														</tbody>
																												</table>
																										</div>
																								</div>
																						</div>
																					</div>
																					
																					
																					

                                        </div>
                                    </div>
                                   
                                    <div class="tab-pane fade" id="pills-details" role="tabpanel" aria-labelledby="pills-details-tab">
                                        <div class="row">
                                            
                                          <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
																						<div class="card">
																								<h5 class="card-header">Tickets</h5>
																								<div class="card-body p-0">
																										<div class="table-responsive">
																											<table class="table">
																													<thead class="bg-light">
																															<tr class="border-0">
																																	<th class="border-0">ID</th>
																																	<th class="border-0">Discount</th>
																																	<th class="border-0">Price</th>
																																	<th class="border-0">Currency</th>
																																	<th class="border-0">Purchase Date</th>
																															</tr>
																													</thead>
																													<tbody>   
																															<% if (user.tickets && user.tickets.length) {%>  
																																	<%user.tickets.forEach( (b,i)=>{%>                                          
																																			<tr class="<%= b.isGuest ? "text-primary":"" %> ">
																																					<td><a href="/api/admin/ticket/<%= b._id %> "><%=b._id%></a></td>
																																					<td><%=b.discount%></td>
																																					<td><%=b.price%></td>
																																					<td><%=b.currency%></td>
																																					<td><%=b.date_of_purchase.toLocaleString()%></td>
																																			</tr>

																																			<tr>
																																					<!-- <td colspan="12"><a href="#" class="btn btn-outline-light float-right">View Details</a></td> -->
																																			</tr>
																																			<%})%>
																															<% }%>  


																													</tbody>
																											</table>
																										</div>
																								</div>
																						</div>
																					</div>
																					

                                        </div>

																		</div>
																		
                                    <div class="tab-pane fade" id="pills-guests" role="tabpanel" aria-labelledby="pills-guests-tab">
                                        <!-- ==================================================== -->
																							<!-- Events -->
																				<!-- ==================================================== -->
                                        <div class="row">
																					<div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
																						<div class="card">
																								<h5 class="card-header">Events</h5>
																								<div class="card-body p-0">
																										<div class="table-responsive">
                                                      <table class="table">
                                                          <thead class="bg-light">
                                                              <tr class="border-0">
                                                                  <th class="border-0">Name</th>
                                                                  <th class="border-0">Description</th>
                                                                  <th class="border-0">Location</th>
                                                                  <th class="border-0">Start</th>
                                                                  <th class="border-0">End</th>
                                                                  <th class="border-0">Entry Fee</th>
                                                                  <th class="border-0">Categories</th>
                                                              </tr>
                                                          </thead>
                                                          <tbody>   
                                                              <% if(user.events_attended && user.events_attended.length) {%>  
                                                                  <%user.events_attended.forEach( (b,i)=>{%>                                          
                                                                      <tr>
                                                                          <td><a href="/api/admin/event/<%= b._id %>"><%= b.name %></a></td>
                                                                          <td><%=b.description%></td>
                                                                          <td><%=b.address.city +", " + b.address.state+", "+b.address.country%></td>
                                                                          <td><%=b.start_date.toLocaleString()%></td>
                                                                          <td><%=b.end_date.toLocaleString()%></td>
                                                                          <td><%=b.entry_fee%></td>
                                                                          <td><%=b.categories%></td>
                                                                      </tr>

                                                                      <tr>
                                                                          <!-- <td colspan="12"><a href="#" class="btn btn-outline-light float-right">View Details</a></td> -->
                                                                      </tr>
                                                                      <%})%>
                                                              <% }%>  


                                                          </tbody>
                                                      </table>
																										</div>
																								</div>
																						</div>
																					</div>
																					

                                        </div>
                                    </div>
                                   
                                </div>
                            </div>
                            
                            <!-- end campaign tab one -->
                            
                        </div>
                        
                        <!-- end campaign data -->
                        
            </div>
        </div>
	</div>
</div>
