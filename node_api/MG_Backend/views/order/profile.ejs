<%- include('../utils/header') %>

<!-- wrapper  -->

<div class="dashboard-wrapper">
  <div class="dashboard-ecommerce">
    <div class="container-fluid dashboard-content">
      <!-- pageheader  -->

      <div class="row">
        <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
          <div class="page-header">
            <h2 class="pageheader-title"></h2>

            <div class="page-breadcrumb">
              <nav aria-label="breadcrumb">
                <ol class="breadcrumb">
                  <li class="breadcrumb-item">
                    <a href="/api/order/:id/details" class="breadcrumb-link"
                      >Dashboard</a
                    >
                  </li>
                  <li class="breadcrumb-item active" aria-current="page">
                    Orders
                  </li>
                </ol>
              </nav>
            </div>
          </div>
        </div>
      </div>
      <!-- end pageheader  -->

      <!-- <p> </p> -->

      <div class="row">
        <!-- profile -->

        <div class="col-xl-6 col-lg-6 col-md-12 col-sm-12 col-12">
          <!-- card profile -->

          <div class="card text-center">
             <div class="card-body">
              <div class="text-center">
                <h2 class="font-24 mb-0"><%=order.name%></h2>
                <h4 class="mb-0"><%= order.item %></h4>
              </div>
            </div>
        
            <div class="card-body border-top">
                <div class="row">
                        <div class="col-6">
              <h3 class="font-16">Merchant</h3>
              <div class="card-text"><%= order.merchant %></div>
              </div>
              <div class="col-6">
                <h3 class="font-16">Client</h3>
                <div class="card-text"><%= order.client %></div>
              </div>
              <div class="col-6">
                <h3 class="font-16">Rider</h3>
                <div class="card-text"><%= order.rider %></div>
              </div>
            </div>
           
           
                
          
            
              <div class="card-body border-top">
                <div class="row text-center justify-content-end">
                  <div class="col-12">
                    <td>
                      <a
                        href="/order/<%= order._id %>/delete"
                        class="btn btn-danger w-50"
                        >Delete</a>
                    </td>
                  </div>
                </div>
              </div>
            
